<p id="notice"><%= notice %></p>

<h1>WynBiz</h1>

<table>
  <thead>
    <tr>
      <!-- <th>Title</th>
      <th>Pic</th>
      <th colspan="3"></th> -->
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><%= picture.title %></td>
       <td><%=  link_to image_tag(picture.url), picture_path(picture)%></td>
        <td id="show"><%= link_to 'Show', picture_path(picture) %></td>
        <td id="edit"><%= link_to 'Edit', edit_picture_path(picture) %></td>
        <td id="destroy"><%= link_to 'Destroy', picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

